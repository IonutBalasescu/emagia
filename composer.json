{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=7.4.3",
    "ext-json": "*",
    "ext-simplexml": "*",
    "doctrine/collections": "^1.6",
    "phpunit/phpunit": "^7.2.0"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    },
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  }
}
